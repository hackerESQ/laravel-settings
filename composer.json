{
    "name": "hackeresq/laravel-settings",
    "description": "Super simple key/value settings for Laravel 5+",
    "version": "3.0.0",
    "keywords": [
        "settings",
        "laravel",
        "key",
        "value"
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "hackerESQ\\Settings\\": "src/"
        }
    },
    "require": {
        "php" : ">=7.0",
        "laravel/framework": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0",
        "doctrine/dbal": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "hackerESQ\\Settings\\SettingsServiceProvider"
            ],
            "aliases": {
                "Settings": "hackerESQ\\Settings\\Facades\\Settings"
            }
        }
    }
}
